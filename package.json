  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "precommit": "lint-staged && ng lint",
    "prepush": "ng test --single-run --browsers ChromeHeadless --watch=false --code-coverage",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test --browsers Chrome",
    "test-auto": "ng test --single-run --browsers ChromeHeadless",
